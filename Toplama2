#include <stdio.h>
int main()
{
    int sayi,toplam=0;
    printf("LÃ¼tfen rakam haricinde sayi giriniz.");
    while(1)
    {
        scanf("%d",&sayi);
        if(sayi>=10)
        {
            toplam += sayi;
        }
        else
        {
            printf("Sayilarin toplami : %d",toplam);
        }
    }
    return 0;
}
